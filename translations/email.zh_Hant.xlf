        <target state="translated">此連結僅在接下來的15分鐘內有效。</target>
      </trans-unit>
      <trans-unit id="8jDJmrK" resname="automated_email_dont_answer" xml:space="preserve" approved="yes">
        <source>automated_email_dont_answer</source>
